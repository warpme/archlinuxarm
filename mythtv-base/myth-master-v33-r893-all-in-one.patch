+static struct drm_v4l2_pix_fmt_mapping drm_v4l2_pix_fmt_map[] = {
+    { .drm_format = DRM_FORMAT_NV12,                .v4l2_pix_fmt = V4L2_PIX_FMT_NV12             },
+    { .drm_format = DRM_FORMAT_NV21,                .v4l2_pix_fmt = V4L2_PIX_FMT_NV21             },
